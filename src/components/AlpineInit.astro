<script>
  if (!(window as any).Alpine) {
    Promise.all([import("alpinejs"), import("@alpinejs/intersect")]).then(
      ([alpine, intersect]) => {
        const Alpine = alpine.default;
        Alpine.plugin(intersect.default);
        (window as any).Alpine = Alpine;
        Alpine.start();
      }
    );
  }
</script>
